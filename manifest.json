{
  "manifest_version": 2, 
  "name": "MatterMost Highlight Monitor", 
  "description": "Highlight monitor and more",
  "version": "1.0",
  "author": "David O Neill",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "fart1.mp3",
    "fart2.mp3",
    "fart3.mp3",
    "fart4.mp3",
    "fart5.mp3",
    "fart6.mp3",
    "fart7.mp3",
    "fart8.mp3",
    "flush.mp3",
    "facebook.mp3",
    "up.png",
    "down.png",
    "alert.png",
    "settings.png",
    "play.png",
    "delete.png",
    "add.png",
    "internet.mp3"
  ],
  "browser_action": {
    "default_title": "MatterMost Highlight Monitor"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "oauth2": {
    "client_id": "109073653369-6umdt06ur5jn9cu80giu4emqh2ej9l36.apps.googleusercontent.com",
    "scopes": [
        "https://www.googleapis.com/auth/drive.appdata"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com https://cdn.rudderlabs.com/ https://*.rudderlabs.com/ 'unsafe-inline'; object-src 'self'"
}
